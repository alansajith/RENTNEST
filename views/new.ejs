<% layout("/Layouts/boilerplate")%>
<body>
    <h5>Create new listing</h5>
    <form method="post" action="/listings">
        <input name="listing[title]" placeholder="Enter title" type="text"><br><br>
        <textarea name="listing[description]" placeholder="Enter description"></textarea><br><br>
        <input name="listing[image]" placeholder="Enter image link" type="text"><br><br>
        <input name="listing[price]" placeholder="Enter price" type="number"><br><br>
        <input name="listing[location]" placeholder="Enter location" type="text"><br><br>
        <input name="listing[country]" placeholder="Enter country" type="text"><br><br>
        <button>Add</button>
    </form>
    
</body>
